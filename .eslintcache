pshot.forEach((doc) => {\r\n          ref.doc(controlBuzzerCode).collection(\"players\").doc(doc.id).update({ hasBuzzedIn: false })\r\n        })\r\n\r\n      })\r\n      \r\n    }\r\n  }\r\n\r\n  return (\r\n    <div className=\"buzzer-control\">\r\n      <h1 id=\"buzzer-code\">Buzzer Code: {controlBuzzerCode}</h1>\r\n\r\n      <button className=\"btn\" onClick={toggleBuzzers}>{buzzerLocked ? \"Unlock Buzzers\" : \"Lock Buzzers\"}</button>\r\n\r\n      <h1 style={{margin: \"20px\"}}>{players.length ? \"Players:\" : \"Nobody yet\"}</h1>\r\n      <div className=\"players\">\r\n        {\r\n          players.map((player) => {\r\n            return (\r\n              <div className=\"player\" key={new Date().getTime() + Math.random()}>\r\n                <h1 style={{color: `hsl(0, 0%, ${600 / ((player.timestamp - timestampUnlocked) / 100)}%)`}}>{player.username}</h1>\r\n                <h2>{player.hasBuzzedIn ? `Buzzed in ${(player.timestamp - timestampUnlocked) / 1000}s after buzzer unlocked` : \"Has not yet buzzed in.\"}</h2>\r\n              </div>\r\n            )\r\n          })\r\n        }\r\n      </div>\r\n\r\n\r\n\r\n    </div>\r\n  )\r\n}\r\n\r\nexport default BuzzerControl\r\n","C:\\Users\\isaac\\Desktop\\dev\\web\\react\\react-buzzer\\src\\pages\\NotFound.js",[],"C:\\Users\\isaac\\Desktop\\dev\\web\\react\\react-buzzer\\src\\firebase.js",[],"C:\\Users\\isaac\\Desktop\\dev\\web\\react\\react-buzzer\\src\\hooks\\useQuery.js",[],"C:\\Users\\isaac\\Desktop\\dev\\web\\react\\react-buzzer\\src\\components\\Modal.js",[],["64","65"],"C:\\Users\\isaac\\Desktop\\dev\\web\\react\\react-buzzer\\src\\reducer.js",[],{"ruleId":"66","replacedBy":"67"},{"ruleId":"68","replacedBy":"69"},{"ruleId":"70","severity":1,"message":"71","line":8,"column":8,"nodeType":"72","messageId":"73","endLine":8,"endColumn":18},{"ruleId":"70","severity":1,"message":"74","line":1,"column":17,"nodeType":"72","messageId":"73","endLine":1,"endColumn":25},{"ruleId":"75","severity":1,"message":"76",